tags:
title: $:/plugins/bimlas/locator/templates/search-toolbar
type: text/vnd.tiddlywiki

\define config-title() $:/config/bimlas/locator/toolbars/search/$(listItem)$

<$vars
  titleOfNewTiddler={{$:/temp/search}}
>
  <$list filter="[all[shadows+tiddlers]tag[$:/tags/bimlas/locator/search-toolbar]!has[draft.of]]" variable="listItem">
    <$reveal type="nomatch" state=<<config-title>> text="hide">
      <$transclude tiddler=<<listItem>> mode="inline"/>
    </$reveal>
  </$list>
</$vars>
