tags:
title: $:/plugins/bimlas/locator/templates/breadcrumbs
type: text/vnd.tiddlywiki

<$set name="ancestorTagTemplate" filter="[<contextState>contains:title[$:/state/bimlas/locator-search]]" value="$:/plugins/bimlas/locator/templates/search-ancestor-tag-list-item" emptyValue="$:/plugins/bimlas/locator/templates/view-ancestor-tag-list-item">
  <$list filter=<<filter-breadcrumbs>>>

    <$list filter="[subfilter<filter-ancestor-tags>is[current]]">
      <$transclude tiddler=<<ancestorTagTemplate>>/>
    </$list>
    <$list filter="[all[current]] -[subfilter<filter-ancestor-tags>]">
      {{||$:/plugins/bimlas/locator/buttons/remove-direct-tag-filter}}
      <<tag>>
    </$list>

  </$list>
</$set>
